<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mygrocery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyGrocery">
        <activity
            android:name=".Homeview"
            android:label="@string/title_activity_homeview"
            android:theme="@style/Theme.MyGrocery.NoActionBar"></activity>
        <activity
            android:name=".Listview"
            android:label="@string/title_activity_listview"
            android:theme="@style/Theme.MyGrocery.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>